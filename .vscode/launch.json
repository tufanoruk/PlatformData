    "version": "0.2.0",
    "configurations": [
        {
            "name": "Node: Launch Platform Data ws",
            "program": "${workspaceFolder}/ws_platformData.js",
            "request": "launch",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "type": "pwa-node"
        },
        {
            "name": "Python: Platform Data",
            "type": "python",
            "request": "launch",
            "program": "dp_platformDataUpdate.py",
            "console": "integratedTerminal"
        },
        {
            "name": "Python: Position Update",
            "type": "python",
            "request": "launch",
            "program": "cl_propPosUpdate.py",
            "console": "integratedTerminal",
            "args": [
                "3N",
                "5E"
            ]
        },
        {
            "name": "Python: Crs & Spd Update",
            "type": "python",
            "request": "launch",
            "program": "cl_propCrsSpdUpdate.py",
            "console": "integratedTerminal",
            "args": [
                "-c 123",
                "-s 5"
            ]
        }
    ]
}